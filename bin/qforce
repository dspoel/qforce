#!/usr/bin/env python3

from qforce.initialize import Initialize
from qforce.parser import parse

print("""\n
   ____         ______
  / __ \       |  ____|
 | |  | |______| |__ ___  _ __ ___ ___
 | |  | |______|  __/ _ \| '__/ __/ _ \\
 | |__| |      | | | (_) | | | (_|  __/
  \___\_\      |_|  \___/|_|  \___\___|

Written by: Selim Sami
University of Groningen - 2020
==============================
""")

args = parse()
inp = Initialize.from_questions("qforce", check_only=True, config=args.o)
inp.setup(args.f)

print("\n")
